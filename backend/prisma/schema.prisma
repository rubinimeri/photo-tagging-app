generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  name    String   @id @unique
  url     String
  objects Object[]
}

model Object {
  name     String  @id
  url      String
  x        Int
  y        Int
  Game     Game?   @relation(fields: [gameName], references: [name])
  gameName String?
}

model Player {
  name      String   @id
  createdAt DateTime @default(now())
  score     Float?
}
